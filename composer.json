{
    "name": "data-house/data-house",
    "type": "project",
    "description": "Data House is a piloting platform to test possible Knowledge Management interventions.",
    "keywords": ["knowledge-management", "pilot", "km"],
    "license": "AGPLv3",
    "require": {
        "php": "^8.3",
        "blade-ui-kit/blade-heroicons": "^2.1",
        "blade-ui-kit/blade-icons": "^1.5",
        "cloudmeshdev/livewire-slideover": "^1.0",
        "codeat3/blade-codicons": "^1.31",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^11.0",
        "laravel/jetstream": "^5.0",
        "laravel/pennant": "^1.5",
        "laravel/pulse": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.1",
        "laravel/tinker": "^2.9",
        "league/flysystem-path-prefixing": "^3.13",
        "league/flysystem-webdav": "^3.15",
        "livewire/livewire": "^3.4",
        "meilisearch/meilisearch-php": "^1.1",
        "oneofftech/laravel-language-recognizer": "^0.2.0",
        "oneofftech/parse-client": "^0.2",
        "prinsfrank/standards": "^3.1",
        "rinvex/countries": "^9.0",
        "smalot/pdfparser": "^2.4",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-data": "^4.5",
        "spatie/laravel-query-builder": "^5.8",
        "staudenmeir/laravel-adjacency-list": "^1.23",
        "sweetrdf/easyrdf": "^1.16",
        "timokoerber/laravel-one-time-operations": "^1.3",
        "vitorccs/laravel-csv": "^1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "replace": {
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php70": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
